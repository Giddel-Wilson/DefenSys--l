# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database_name?retryWrites=true&w=majority

# JWT Secret Key (Use a strong, random string in production)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Environment
NODE_ENV=development

# Resend Email API Configuration (Optional - for admin notifications)
# Sign up at https://resend.com to get your API key (free tier: 3,000 emails/month)
# If not configured, email notifications will be logged to console instead
RESEND_API_KEY=re_your_api_key_here
RESEND_FROM_EMAIL=onboarding@resend.dev
